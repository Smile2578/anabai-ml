# anabai-ml/.env.example

# Base de données PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=anabai
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password
DATABASE_URL=postgresql+asyncpg://postgres:your_password@localhost:5432/anabai

# Base de données MongoDB
MONGODB_URI=mongodb+srv://user:password@your-cluster.mongodb.net/anabai-ml

# Redis (Upstash)
REDIS_HOST=your-redis-host
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_SSL=true
REDIS_URL=rediss://your-redis-host:6379

# Redis ML (Upstash)
REDIS_ML_HOST=your-redis-host
REDIS_ML_PORT=6379
REDIS_ML_PASSWORD=your_redis_password
REDIS_ML_SSL=true
REDIS_ML_URL=rediss://your-redis-host:6379
REDIS_ML_DB=0

# APIs externes
OPENWEATHER_API_KEY=your_openweather_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
GOOGLE_MAPS_SERVER_KEY=your_google_maps_server_key

# Chemins des modèles et cache
ML_MODELS_PATH=./models
TENSORFLOW_CACHE_DIR=./cache/tensorflow
TORCH_HOME=./cache/torch
TRANSFORMERS_CACHE=./cache/transformers

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/anabai.log

# Environnement
ENVIRONMENT=development

# Limites et timeouts
MAX_PLACES_PER_ITINERARY=12
MAX_ITINERARIES_PER_USER=5
MAX_REQUESTS_PER_MINUTE=60
REQUEST_TIMEOUT=30

# Sécurité
SECRET_KEY=your_secret_key
JWT_SECRET=your_jwt_secret
JWT_ALGORITHM=HS256
JWT_EXPIRATION=3600

# Configuration de la base de données
DATABASE_URL=postgresql://user:password@localhost:5432/anabai
MONGODB_URL=mongodb://localhost:27017/anabai
REDIS_URL=redis://localhost:6379/0

# Clés API
OPENWEATHER_API_KEY=your_openweather_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Configuration de l'application
APP_ENV=development
DEBUG=true
LOG_LEVEL=debug

# Configuration des tests
TEST_DATABASE_URL=postgresql://user:password@localhost:5432/anabai_test
TEST_MONGODB_URL=mongodb://localhost:27017/anabai_test
TEST_REDIS_URL=redis://localhost:6379/1